{
  "name": "signix",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettify": "prettier \"./src/**/*.{ts,tsx,css,md,json}\" --write",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,css,md,json}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint",
      "jest --findRelatedTests"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.7.1",
    "@material-ui/icons": "^4.5.1",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.12",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "deepmerge": "^4.2.2",
    "downshift": "^3.4.8",
    "install": "^0.13.0",
    "laco": "^1.2.1",
    "laco-react": "^1.1.0",
    "lodash": "^4.17.15",
    "npm": "^6.13.4",
    "react": "^16.12.0",
    "react-color": "^2.17.3",
    "react-cursor-position": "^3.0.3",
    "react-custom-scrollbars": "^4.2.1",
    "react-dnd": "^9.5.1",
    "react-dnd-html5-backend": "^9.5.1",
    "react-dom": "^16.12.0",
    "react-draggable": "^4.2.0",
    "react-json-view": "^1.19.1",
    "react-scripts": "^3.3.0",
    "react-slider": "^1.0.1",
    "react-sortly": "^3.0.4",
    "react-split-pane": "^0.1.89",
    "react-use": "^13.10.0",
    "store": "^2.0.12",
    "styled-components": "^4.4.1",
    "typescript": "3.7.2"
  },
  "devDependencies": {
    "@egoist/vue-to-react": "^1.1.0",
    "@storybook/addon-actions": "^5.3.0-beta.6",
    "@storybook/addon-docs": "^5.3.0-beta.6",
    "@storybook/addon-links": "^5.3.0-beta.6",
    "@storybook/addons": "^5.3.0-beta.6",
    "@storybook/core": "^5.2.8",
    "@storybook/preset-create-react-app": "^1.3.2",
    "@storybook/react": "^5.3.0-beta.18",
    "@types/lodash": "^4.14.149",
    "@types/react-color": "^3.0.1",
    "@types/react-custom-scrollbars": "^4.0.6",
    "@types/react-slider": "^1.0.0",
    "@types/store": "^2.0.2",
    "@types/styled-components": "^4.4.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "vue": "^2.6.10"
  }
}
